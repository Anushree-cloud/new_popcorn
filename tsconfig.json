{
    "compilerOptions": {
        "sourceMap": true,
        "module": "commonJS",
        "rootDir":"./src",
        "outDir":"./build",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        // "strict":true,
        "skipLibCheck": true,
        "lib":[
            "ESNext"
        ]

    },
    "include": [
        "src/**/*"
    ],
    "exclude":[
        "node_modules",
        "**/*.spec.ts",
        "build"
    ]
}